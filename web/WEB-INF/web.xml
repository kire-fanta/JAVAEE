<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MonServletJsp</servlet-name>
        <servlet-class>kire.fanta.MonServletJsp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonServlet</servlet-name>
        <servlet-class>Com.kire.MonServletJsp.MonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>monservlet2.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonServlet2</servlet-name>
        <servlet-class>Com.kire.MonServletJsp.MonServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MonServletJsp</servlet-name>
        <url-pattern>/MonServletJsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonServlet</servlet-name>
        <url-pattern>/MonServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonServlet2</servlet-name>
        <url-pattern>/MonServlet2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
